{
  "name": "ftc-maintenance-sripts",
  "repository": "https://github.com/technototes/PowerPlay2022.git",
  "author": "Kevin Frei <kevinfrei@hotmail.com>",
  "license": "CC0-1.0",
  "type": "module",
  "scripts": {
    "- // a": "These are all for fast building 'stuff':",
    "fast": "yarn gradle :Twenty403:assembleDebug :Sixteen750:assembleDebug",
    "fast16": "yarn gradle :Sixteen750:assembleDebug",
    "fast20": "yarn gradle :Twenty403:assembleDebug",
    "fastlb": "yarn gradle :LearnBot:assembleDebug",
    "- // b": "These are all for 'fullly' building 'stuff':",
    "build": "yarn gradle :Sixteen750:build :Twenty403:build",
    "build16": "yarn gradle :Sixteen750:build",
    "build20": "yarn gradle :Twenty403:build",
    "buildlb": "yarn gradle :LearnBot:build",
    "full": "yarn gradle build",
    "- // c": "These scripts are for formatting code/scripts/markdown stuff:",
    "format": "yarn prettier --write Sixteen750/src/**/*.java Twenty403/src/**/*.java helpers-src/**/*.ts */*.md",
    "format16": "yarn prettier --write Sixteen750/src/**/*.java",
    "format20": "yarn prettier --write Twenty403/src/**/*.java",
    "formatlb": "yarn prettier --write LearnBot/src/**/*.java",
    "formatmm": "yarn prettier --write MeepMeepTesting/src/**/*.java",
    "formatjs": "yarn prettier --write *.json helpers-src/**/*.ts */*.md",
    "formatall": "yarn format && yarn formatjs && yarn formatmm && yarn formatlb",
    "- // d": "This will toggle using TechnoLib locally or from Maven",
    "libflip": "yarn node helpers/flip.js lib",
    "- // e": "Disable/enable 'all' bots & subbuilds",
    "botflip": "yarn node helpers/flip.js bot",
    "- // f": "Stuff for connecting/disconnecting from the bot",
    "connect": "yarn adb connect 192.168.43.1:5555",
    "disconnect": "yarn adb disconnect 192.168.43.1:5555",
    "discall": "yarn adb disconnect",
    "- // g": "Open the FTC Dashboard!",
    "dash": "yarn open http://192.168.43.1:8080/dash",
    "- // h": "platform-specific commands",
    "gradle": "yarn run-script-os",
    "gradle:win32": "gradlew",
    "gradle:default": "./gradlew",
    "adb": "yarn run-script-os",
    "adb:win32": "helpers\\\\adb.cmd",
    "adb:macos": "~/Library/Android/sdk/platform-tools/adb",
    "adb:linux": "~/Android/Sdk/platform-tools/adb",
    "open": "yarn run-script-os",
    "open:win32": "start \"\"",
    "open:macos": "open",
    "open:linux": "xdg-open",
    "- // i": "build stuff for the MeepMeep thing",
    "meep": "yarn node helpers/preprocessor.js",
    "testmeep": "yarn node helpers/preprocessor.js /Users/freik/src/ftc/CenterStage2023/MeepMeepTesting/build '[/Users/freik/src/ftc/CenterStage2023/Sixteen750/src/main/java/org/firstinspires/ftc/sixteen750/AutoConstants.java, /Users/freik/src/ftc/CenterStage2023/LearnBot/src/main/java/org/firstinspires/ftc/learnbot/opmodes/BasicAuto.java, /Users/freik/src/ftc/CenterStage2023/MeepMeepTesting/src/main/java/com/example/meepmeeptesting/AutoConstantsBlue.java, /Users/freik/src/ftc/CenterStage2023/MeepMeepTesting/src/main/java/com/example/meepmeeptesting/AutoConstantsRed.java, /Users/freik/src/ftc/CenterStage2023/RoadRunnerQuickStart/src/main/java/org/firstinspires/ftc/teamcode/drive/opmode/AutomaticFeedforwardTuner.java, /Users/freik/src/ftc/CenterStage2023/Twenty403/src/main/java/org/firstinspires/ftc/twenty403/AutoConstants.java]'",
    "- // j": "This is the script that presents the students with a workflow",
    "work": "yarn node helpers/workflow.js",
    "- // z": "Build stuff for all the scripts in helpers",
    "types": "yarn tsc --noEmit",
    "tsc": "yarn tsc"
  },
  "dependencies": {
    "@freik/containers": "^0.2.13",
    "@freik/typechk": "^0.5.11",
    "@types/node": "^20.14.6",
    "java-parser": "^2.3.0",
    "prettier": "^3.3.2",
    "prettier-plugin-java": "^2.6.0",
    "run-script-os": "^1.1.6",
    "simple-git": "^3.25.0",
    "typescript": "^5.4.5"
  },
  "packageManager": "yarn@4.3.0",
  "dependenciesMeta": {
    "prettier-plugin-java@2.6.0": {
      "unplugged": true
    }
  }
}
